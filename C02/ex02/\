#include <unistd.h>

int	ft_str_is_apha(char *str)
{
	int	i;
	int	n;


	



	while((i < n) && (i <= '9'))
	{
		while((i < n) && (i <= 'z'))
		{
			while((i < n) && (i == '\0))
			{
				write(1, '1', 1);
			}
			write(1, '1', 1);
		}
		write(1, '0', 1);
	}
	return (0);
}


